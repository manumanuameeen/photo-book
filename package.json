    "@types/morgan": "^1.9.10",
    "concurrently": "^8.2.2",
    "morgan": "^1.10.1"
  },
  "dependencies": {
    "@types/redis": "^4.0.10",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.0",
    "jsonwebtoken": "^9.0.2",
    "redis": "^5.9.0"
